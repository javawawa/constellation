<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.twosmall.constellation.mapper.EnglishWordMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.twosmall.constellation.entity.dao.EnglishWordDao">
        <result column="id" property="id" />
        <result column="word" property="word" />
        <result column="phonogram" property="phonogram" />
        <result column="paraphrase" property="paraphrase" />
        <result column="study_status" property="studyStatus" />
        <result column="note_flag" property="noteFlag" />
        <result column="audio_path_en" property="audioPathEn" />
        <result column="audio_path_am" property="audioPathAm" />
        <result column="create_time" property="createTime" />
        <result column="create_people" property="createPeople" />
        <result column="modify_time" property="modifyTime" />
        <result column="modify_people" property="modifyPeople" />
        <result column="deleted" property="deleted" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id,
        word, phonogram, paraphrase, study_status, note_flag, audio_path_en, audio_path_am, create_time, create_people, modify_time, modify_people, deleted
    </sql>

</mapper>
